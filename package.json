    "codemirror": "^5.38.0",
    "compression": "^1.7.2",
    "express": "^4.16.3",
    "form-serialize": "^0.7.2",
    "isomorphic-fetch": "^2.2.1",
    "jss": "^9.8.1",
    "jss-preset-default": "^4.3.0",
    "mobx": "^4.1.1",
    "mobx-react": "^5.0.0",
    "next": "^6.0.3-canary.1",
    "next-routes": "^1.4.1",
    "npm-run-all": "^4.1.3",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-codemirror2": "^5.0.4",
    "react-dom": "^16.3.2",
    "react-jss": "^8.4.0",
    "recompose": "^0.26.0",
    "socket.io-client": "^2.1.1",
    "styled-components": "^3.2.5",
    "styled-jsx": "^2.2.6",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-plugin-direct-import": "^0.6.0-beta.1",
