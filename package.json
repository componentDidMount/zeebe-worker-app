{
  "name": "zeebe-worker-app",
  "version": "1.0.0",
  "description": "zeebe-worker-app",
  "main": "server.js",
  "scripts": {
    "watch:render": "node server/render/index.js",
    "watch:rest": "cd server/rest && npm run watch",
    "start:render": "NODE_ENV=production node server/render/index.js",
    "start:rest": "cd server/rest && npm start",
    "watch": "npm-run-all --parallel watch:render watch:rest",
    "build": "next build src",
    "start": "npm-run-all --parallel start:render start:rest",
    "zeebe-rm": "docker rm -f zeebe",
    "zeebe-start": "docker run --name zeebe -e ZEEBE_HOST=0.0.0.0 -p 51015:51015 camunda/zeebe:latest",
    "docker": "npm install && npm run build && npm start"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
